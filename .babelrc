{
    "presets": [
        [ "es2015" ], "react", "stage-0"
    ],
    "plugins": [
        ["module-resolver", {
            "root": ["./src"]
        }]
    ],
    "env": {
        "test": {
            "plugins": [
                ["import-noop", {
                    "extensions": ["less", "css"]
                }]
            ]
        }
    }
}
